# Configuration de l'application
PORT=3000
NODE_ENV=development
JWT_SECRET=supply_chain_secret_key

# Configuration PostgreSQL principale
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=supply_chain_db
POSTGRES_USER=supply_chain_user
POSTGRES_PASSWORD=supply_chain_password

# Configuration PostgreSQL IA
POSTGRES_IA_HOST=localhost
POSTGRES_IA_PORT=5433
POSTGRES_IA_DB=IA_supply
POSTGRES_IA_USER=ia_user
POSTGRES_IA_PASSWORD=ia_password

# Configuration MongoDB
MONGODB_URL=mongodb://supply_chain_user:supply_chain_password@localhost:27017/supply-IA

# Configuration Redis
REDIS_URL=redis://localhost:6379

# Configuration ClickHouse
CLICKHOUSE_URL=http://localhost:8123
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=

# Configuration MQTT (pour le suivi des véhicules)
MQTT_BROKER_URL=mqtt://localhost:1883

# Configuration des services externes
EMAIL_SERVICE_API_KEY=your_email_service_api_key
SMS_SERVICE_API_KEY=your_sms_service_api_key

# Configuration de l'IA
AI_MODEL_STORAGE_PATH=./models/storage
AI_TEMP_PATH=./temp

# Configuration de logging
LOG_LEVEL=info
LOG_FILE_PATH=./logs/app.log

# Configuration de sécurité
RATE_LIMIT_WINDOW_MS=900000 # 15 minutes
RATE_LIMIT_MAX_REQUESTS=100 # 100 requêtes par fenêtre